<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane hgap="5.0" minHeight="300.0" minWidth="300.0" style="-fx-background-color: rgb(31,31,31);" vgap="5.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1"
          fx:controller="Controller.Menus.GameConclusionWindowController">
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="50.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" percentWidth="50.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" onMouseClicked="#closeStage"
           onMouseEntered="#mouseIsOver" onMouseExited="#mouseIsOut"
           style="-fx-background-image: url(&quot;https://i.imgur.com/9ki490h.png&quot;); -fx-background-radius: 20; -fx-background-size: 40;"
           GridPane.halignment="LEFT" GridPane.valignment="TOP"/>
    <Label fx:id="conclusion" alignment="CENTER" minHeight="50.0" minWidth="300.0" text="-" textFill="#00ffa8"
           GridPane.columnSpan="2" GridPane.rowIndex="1">
        <font>
            <Font name="American Typewriter" size="39.0"/>
        </font>
        <effect>
            <Glow/>
        </effect>
    </Label>
    <ImageView fx:id="pfp1" fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true"
               GridPane.rowIndex="2"/>
    <ImageView fx:id="pfp2" fitHeight="75.0" fitWidth="75.0" layoutX="77.0" layoutY="208.0" pickOnBounds="true"
               preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <Label fx:id="username1" text="-" textFill="WHITE" GridPane.rowIndex="3">
        <effect>
            <Glow/>
        </effect>
        <font>
            <Font name="Arial" size="17.0"/>
        </font>
    </Label>
    <Label fx:id="username2" layoutX="99.0" layoutY="273.0" text="-" textFill="WHITE" GridPane.columnIndex="1"
           GridPane.rowIndex="3">
        <effect>
            <Glow/>
        </effect>
        <font>
            <Font name="Arial" size="17.0"/>
        </font>
    </Label>
    <Label fx:id="score1" layoutX="93.0" layoutY="270.0" text="-" textFill="#fbff00" GridPane.rowIndex="4">
        <effect>
            <Glow/>
        </effect>
        <font>
            <Font name="American Typewriter" size="34.0"/>
        </font>
    </Label>
    <Label fx:id="score2" layoutX="93.0" layoutY="275.0" text="-" textFill="#fbff00" GridPane.columnIndex="1"
           GridPane.rowIndex="4">
        <effect>
            <Glow/>
        </effect>
        <font>
            <Font name="American Typewriter" size="34.0"/>
        </font>
    </Label>
</GridPane>
